{
  "name": "@rxstack/mongoose-service",
  "version": "0.1.6",
  "description": "RxStack Mongoose Service",
  "private": false,
  "author": "Nikolay Georgiev <symfonist@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/rxstack/mongoose-service.git"
  },
  "engines": {
    "node": ">=9"
  },
  "keywords": [
    "rxstack",
    "mongodb",
    "mongoose"
  ],
  "main": "dist/index",
  "typings": "dist/index",
  "scripts": {
    "prepublishOnly": "npm run compile",
    "compile": "npm run clean && tsc -p .",
    "clean": "rm -rf dist",
    "lint": "tslint --project tsconfig.json --format stylish",
    "mocha": "mocha --opts mocha.opts",
    "coverage": "nyc npm run mocha",
    "test": "npm run lint && npm run coverage"
  },
  "peerDependencies": {
    "@rxstack/core": "^0.1",
    "@rxstack/exceptions": "^0.1",
    "@rxstack/platform": "^0.1",
    "@rxstack/query-filter": "^0.1",
    "@rxstack/security": "^0.1"
  },
  "dependencies": {
    "@rxstack/core": "^0.1.2",
    "@rxstack/exceptions": "^0.1.5",
    "@rxstack/platform": "^0.1.10",
    "@rxstack/query-filter": "^0.1.2",
    "@rxstack/security": "^0.1.7",
    "@types/mongoose": "^5.3.21",
    "injection-js": "^2.1",
    "mongodb": "^3.1.13",
    "mongoose": "^5.4.18",
    "reflect-metadata": "^0.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/lodash": "^4.14.122",
    "@types/mocha": "^2.2.48",
    "@types/node": "^11.11.0",
    "chai": "3.5.0",
    "mocha": "^5.2.0",
    "nyc": "^13.3.0",
    "should": "^13.2.3",
    "sinon": "^6.3.4",
    "source-map-support": "^0.4.15",
    "ts-node": "^3.1.0",
    "tslint": "^5.13.1",
    "typescript": "^3.3.3333",
    "uuid": "^3.3.2"
  },
  "bugs": {
    "url": "https://github.com/rxstack/mongoose-service/issues"
  },
  "homepage": "https://github.com/rxstack/mongoose-service",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "nyc": {
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text-summary",
      "html",
      "lcov"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
